{
    "license": "MIT",
    "private": true,
    "main": "webpack.config.js",
    "version": "1.0.0",
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && mkdir dist && webpack --progress --output-path ./dist --mode production",
        "build:vite": "npm run clean && vite build",
        "start": "webpack-dev-server -d --hot --progress --colors --https --content-base ./src/ --port 8888",
        "start:vite": "vite",
        "package": "npm run build && node buildPackages.js",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "type-check": "tsc --noEmit",
        "test": "echo \"No tests specified\" && exit 0",
        "test:watch": "echo \"No tests specified\" && exit 0",
        "security-audit": "npm audit",
        "security-fix": "npm audit fix",
        "update-deps": "npm update && npm audit fix"
    },
    "devDependencies": {
        "@types/jquery": "^3.5.0",
        "@types/lodash": "^4.14.200",
        "@types/react": "^16.14.0",
        "@types/react-dom": "^16.9.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.0",
        "eslint": "^8.0.0",
        "eslint-plugin-react": "^7.33.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "rimraf": "^5.0.0",
        "sass": "^1.69.0",
        "sass-loader": "^13.3.0",
        "sass-resources-loader": "^2.2.0",
        "style-loader": "^3.3.0",
        "terser-webpack-plugin": "^5.3.0",
        "tfx-cli": "^0.21.1",
        "ts-loader": "^9.5.0",
        "typescript": "^4.9.0",
        "webpack": "^5.89.0",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2",
        "vite": "^5.0.0",
        "@vitejs/plugin-react": "^4.2.0"
    },
    "dependencies": {
        "@fluentui/react": "^8.110.0",
        "azure-devops-extension-sdk": "^2.0.0",
        "azure-devops-ui": "^2.167.0",
        "date-fns": "^2.30.0",
        "es6-promise": "^4.2.8",
        "lodash": "^4.17.21",
        "markdown-it": "^13.0.0",
        "office-ui-fabric-react": "^7.199.1",
        "react-sortable-hoc": "^2.0.0",
        "recharts": "^2.8.0",
        "roosterjs": "^8.61.0",
        "tslib": "^2.6.0",
        "vss-ui": "^4.134.1",
        "vss-web-extension-sdk": "^5.134.0"
    }
}
